{
  "name": "custom-d/eloquent-model-encrypt",
  "description": "A eloquent model encrypt",
  "license": "MIT",
  "keywords": [
    "eloquent",
    "model",
    "encrypt"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Craig Smith",
      "email": "craig.smith@customd.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^5.8|^6.0|^7.0|^8.0",
    "custom-d/eloquent-async-keys": "^2.3.2"
  },
  "require-dev": {
    "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "CustomD\\EloquentModelEncrypt\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CustomD\\EloquentModelEncrypt\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "CustomD\\EloquentModelEncrypt\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
